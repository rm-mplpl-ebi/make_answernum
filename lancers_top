<script type="text/javascript">
    function random(len){
        var l = len;
        var c = "012345679";
        var cl = c.length;
        var r = "";
        for(var i = 0; i<8; i++){
            r += c[Math.floor(Math.random() * cl)];
        }
        document.getElementById("password").value = r;
    }

</script>

    <form>
    <input type="text" id="password" readonly>
	<input type="button" value="回答者番号を生成" id="button1" onclick="random(8), this.disabled = true;"/>
    </form>
